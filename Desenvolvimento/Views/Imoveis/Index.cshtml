@model IEnumerable<Desenvolvimento.Models.Imoveis>

@{
    ViewBag.Title = "Lista de Imóveis";
}

<h2>Lista de Imóveis</h2>

<p class="pull-right">
    @Html.ActionLink("Criar Novo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tipo_negocio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descricao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cpf_cliente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nome_cliente)
        </th>  
        <th>
            @Html.DisplayNameFor(model => model.valor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ativo)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.tipo_negocio)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.descricao)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.cpf_cliente)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.nome_cliente)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.valor)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ativo)
    </td>
    <td>
        @Html.ActionLink("Editar", "Edit", new { id = item.ID }) |
        @Html.ActionLink("Detalhes", "Details", new { id = item.ID }) |
        @Html.ActionLink("Deletar", "Delete", new { id = item.ID })
    </td>
</tr>
    }

</table>
